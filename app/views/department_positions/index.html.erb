<p id="notice"><%= notice %></p>

<h1>Department positions</h1>

<table>
  <thead>
    <tr>
      <th>Department</th>
      <th>Position</th>
      <th>Decription</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @department_positions.each do |department_position| %>
      <tr>
        <td><%= department_position.department_id %></td>
        <td><%= department_position.position_id %></td>
        <td><%= department_position.decription %></td>
        <td><%= link_to 'Show', department_position %></td>
        <td><%= link_to 'Edit', edit_department_position_path(department_position) %></td>
        <td><%= link_to 'Destroy', department_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Department position', new_department_position_path %>
